{
  "manifest_version": 3,
  "name": "GoTo",
  "version": "1.2.0",
  "description": "Quickly GoTo Tabs, Bookmarks, and History",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "history",
    "bookmarks",
    "storage",
    "contextMenus"
  ],
  "action": {
    "default_title": "GoTo",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "toggle-search": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Toggle search window"
    }
  }
}